{
  "name": "stocks-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=3500 nodemon --exec babel-node src/bin/server.js",
    "build": "babel src --out-dir dist",
    "serve": "node dist/bin/server.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongodb": "^3.1.10",
    "monk": "^6.0.6",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.9"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "2500"
  }
}
